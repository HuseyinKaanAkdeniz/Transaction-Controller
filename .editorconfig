[*.cs]

# CS8618: Non-nullable field must contain a non-null value when exiting constructor. Consider declaring as nullable.
dotnet_diagnostic.CS8618.severity = silent

# CS0649: Field 'ApplicationDbContext.Categories' is never assigned to, and will always have its default value null
dotnet_diagnostic.CS0649.severity = none

# CS0472: The result of the expression is always the same since a value of this type is never equal to 'null'
dotnet_diagnostic.CS0472.severity = silent
